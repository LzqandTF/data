<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="BankInst"
		table="bank_inst"
		lazy="false"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="bank_id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="BankName"
			column="bank_name"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="ParseDzFileClass"
			column="parse_dz_file_class"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="TradeDzImplClass"
			column="trade_dz_impl_class"
			type="string"
			length="64"
		/>
		<property
			name="DzDataTableName"
			column="dz_data_tableName"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="FtpDzFilePath"
			column="ftp_dz_file_path"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="DzFileNamePattern"
			column="dz_file_name_pattern"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="DzFilePath"
			column="dz_file_path"
			type="string"
			not-null="true"
			length="128"
		/>
		<property
			name="OriginalDataTableName"
			column="original_data_tableName"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="RiqieOriginalTableName"
			column="riqie_original_tableName"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="TkTableName"
			column="tk_tableName"
			type="string"
			length="64"
		/>
		<property
			name="InstEntityName"
			column="inst_entity_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="BankEntityName"
			column="bank_entity_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="StartRow"
			column="start_row"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="IsTk"
			column="isTk"
			type="boolean"
			not-null="true"
		/>
		<property
			name="TkType"
			column="tk_type"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TkContext"
			column="tk_context"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="TkColumn"
			column="tk_column"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BankType"
			column="bank_type"
			type="integer"
		/>
		<property
			name="WhetherOuterDz"
			column="whether_outer_dz"
			type="boolean"
		/>
		<set name="InstInfos" inverse="true" lazy="false">
			<key column="bank_id"/>
			<one-to-many class="InstInfo"/>
		</set>


	</class>	
</hibernate-mapping>