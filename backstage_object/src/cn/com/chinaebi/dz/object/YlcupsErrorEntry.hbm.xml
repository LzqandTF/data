<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="YlcupsErrorEntry"
		table="ylcups_error_entry"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="id"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>

		<property
			name="OutAccount"
			column="out_account"
			type="string"
			not-null="true"
			length="19"
		/>
		<property
			name="MerName"
			column="mer_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="TradeTime"
			column="trade_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="TradeResult"
			column="trade_result"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DeductSysReference"
			column="deduct_sys_reference"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="ReqSysStance"
			column="reqSysStance"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="TradeAmount"
			column="tradeAmount"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="HandlingId"
			column="handling_id"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="AcqInstIdCode"
			column="acqInstIdCode"
			type="string"
			not-null="false"
			length="11"
		/>
		<property
			name="ReasonCode"
			column="reason_code"
			type="string"
			not-null="true"
			length="4"
		/>
		<property
			name="DeductStlmDate"
			column="deductStlmDate"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="TradeCategory"
			column="trade_category"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="DeductSysId"
			column="deduct_sys_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TradeSource"
			column="trade_source"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="TradeStatus"
			column="trade_status"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TurndownRemark"
			column="turnDown_remark"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="EnteringTime"
			column="entering_time"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Operator"
			column="operator"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="AuditOperator"
			column="audit_operator"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="CommitTime"
			column="commit_time"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Process"
			column="process"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="TradeMsgType"
			column="tradeMsgType"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BkChk"
			column="bk_chk"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="InstType"
			column="inst_type"
			type="integer"
		/>

	</class>	
</hibernate-mapping>