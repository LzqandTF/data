<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="CustomObject"
		table="custom_object"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="object_id"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>

		<property
			name="ObjectName"
			column="object_name"
			type="string"
			not-null="true"
			length="124"
		/>
		<property
			name="FileAddress"
			column="file_address"
			type="string"
			not-null="true"
			length="124"
		/>
		<property
			name="DzFileName"
			column="dz_file_name"
			type="string"
			not-null="true"
			length="124"
		/>
		<property
			name="ErrorFileName"
			column="error_file_name"
			type="string"
			not-null="false"
			length="124"
		/>
		<property
			name="WhetherUpload"
			column="whether_upload"
			type="boolean"
			not-null="true"
		/>
		<property
			name="FtpIp"
			column="ftp_ip"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="FtpAddress"
			column="ftp_address"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="FtpPort"
			column="ftp_port"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="FtpUsername"
			column="ftp_username"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="FtpPassword"
			column="ftp_password"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="FileSuffix"
			column="file_suffix"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="GenerateNumber"
			column="generate_number"
			type="int"
			not-null="true"
			length="1"
		/>
		<property
			name="FileNeedOnlineData"
			column="file_need_online_data"
			type="int"
			not-null="false"
			length="1"
		/>
		<property
			name="DataType"
			column="data_type"
			type="int"
			not-null="false"
			length="1"
		/>
		<property
			name="WhetherCreateErrorFile"
			column="whether_create_error_file"
			type="int"
			not-null="false"
			length="1"
		/>
		<property
			name="WhetherCreateFileByRange"
			column="whether_create_file_by_range"
			type="boolean"
			not-null="true"
		/>
		<property
			name="FileType"
			column="file_type"
			type="int"
			not-null="true"
			length="1"
		/>
		<property
			name="WhetherCreateFileByInst"
			column="whether_create_file_by_inst"
			type="boolean"
			not-null="false"
			length="1"
		/>

	</class>	
</hibernate-mapping>