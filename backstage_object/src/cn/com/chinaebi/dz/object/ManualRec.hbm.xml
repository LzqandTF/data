<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="ManualRec"
		table="manual_rec"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="MerAbbreviation"
			column="mer_abbreviation"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="RecAmount"
			column="rec_amount"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="MerBalance"
			column="mer_balance"
			type="string"
			not-null="true"
			length="40"
		/>
		<property
			name="Addorsub"
			column="addorsub"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="HandlerTime"
			column="handler_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="SettleTime"
			column="settle_time"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AuditTime"
			column="audit_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="SendUserName"
			column="send_user_name"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="AuditorUserName"
			column="auditor_user_name"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="DataStatus"
			column="data_status"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RequestDesc"
			column="request_desc"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="AuditDesc"
			column="audit_desc"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="WhetherJs"
			column="whetherJs"
			type="boolean"
			not-null="false"
		/>
		<many-to-one
			name="MerCode"
			column="mer_code"
			class="MerBasic"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>