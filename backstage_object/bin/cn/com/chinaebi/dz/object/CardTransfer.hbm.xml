<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="CardTransfer"
		table="card_transfer"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="card_id"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>

		<property
			name="CardInsDesc"
			column="card_ins_desc"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="CardInsCode"
			column="card_ins_code"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="CardDesc"
			column="card_desc"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="CardAtm"
			column="card_atm"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="CardPos"
			column="card_pos"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="CardTracktwo"
			column="card_tracktwo"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="CardTrackthree"
			column="card_trackthree"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="CardTracktwoLength"
			column="card_tracktwo_length"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="CardTrackthreeLength"
			column="card_trackthree_length"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="CardNoLength"
			column="card_no_length"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CardLength"
			column="card_length"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="CardHead"
			column="card_head"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="CardTypeDesc"
			column="card_type_desc"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="CardType"
			column="card_type"
			type="string"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>