<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="TmpSplitTab"
		table="tmp_split_tab"
	>
		<meta attribute="sync-DAO">true</meta>

		<property
			name="Id"
			column="id"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="TradeTime"
			column="trade_time"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="DeductSysId"
			column="deduct_sys_id"
			type="string"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>