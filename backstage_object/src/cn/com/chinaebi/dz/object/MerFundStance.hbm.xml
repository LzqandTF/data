<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="MerFundStance"
		table="mer_fund_stance"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="id"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>

		<property
			name="TradeTime"
			column="trade_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="TradeAmount"
			column="trade_amount"
			type="java.lang.Double"
			not-null="true"
			length="8"
		/>
		<property
			name="MerFee"
			column="mer_fee"
			type="java.lang.Double"
			not-null="true"
			length="8"
		/>
		<property
			name="ChangeAmount"
			column="change_amount"
			type="java.lang.Double"
			not-null="true"
			length="8"
		/>
		<property
			name="AccountAmount"
			column="account_amount"
			type="java.lang.Double"
			not-null="true"
			length="8"
		/>
		<property
			name="TradeStance"
			column="trade_stance"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="DercStatus"
			column="derc_status"
			type="integer"
			not-null="true"
		/>
		<property
			name="MerState"
			column="mer_state"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="MerCategory"
			column="mer_category"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="MerName"
			column="mer_name"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="InstId"
			column="inst_id"
			type="integer"
			not-null="true"
			length="11"	
		/>
		<property
			name="DeductStlmDate"
			column="deduct_stlm_date"
			type="string"
			not-null="true"		
		/>
		<property
			name="InstType"
			column="inst_type"
			type="integer"
		/>
		<property
			name="StanceTime"
			column="stance_time"
			type="string"
		/>
		<property
			name="BankId"
			column="bank_id"
			type="integer"
			not-null="true"
		/>
		<many-to-one
			name="MerCode"
			column="mer_code"
			class="MerBasic"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>