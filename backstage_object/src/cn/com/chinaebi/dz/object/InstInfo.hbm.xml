<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.chinaebi.dz.object">
	<class
		name="InstInfo"
		table="inst_info"
	>
		<meta attribute="sync-DAO">true</meta>
		<composite-id name="Id" class="InstInfoPK">
			<key-property
				name="InstId"
				column="inst_id"
				type="integer"
			/>
			<key-property
				name="InstType"
				column="inst_type"
				type="integer"
			/>
		</composite-id>

		<property
			name="WhetherInnerDz"
			column="whether_inner_dz"
			type="boolean"
			not-null="true"
		/>
		<property
			name="TradeDzImplClass"
			column="trade_dz_impl_class"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Name"
			column="name_"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="WhetherInnerJs"
			column="whether_inner_js"
			type="boolean"
			not-null="true"
		/>
		<property
			name="WhetherApplyOnlineTk"
			column="whether_apply_online_tk"
			type="boolean"
		/>
		<property
			name="Active"
			column="active"
			type="boolean"
			not-null="true"
		/>
		<property
			name="WhetherOuterErrorDz"
			column="whether_outer_error_dz"
			type="boolean"
			not-null="true"
		/>
		<property
			name="WhetherOuterDz"
			column="whether_outer_dz"
			type="boolean"
			not-null="true"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="ErrorDzDataTablename"
			column="error_dz_data_tableName"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="ErrorOriginalDataTablename"
			column="error_original_data_tableName"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="ReceiviName"
			column="receivi_name"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="DzDataColumn"
			column="dz_data_column"
			type="string"
		/>
		<property
			name="Gate"
			column="gate"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="BankftpDownload"
			column="bankftp_download"
			type="boolean"
			not-null="false"
		/>
		<property
			name="BankftpPath"
			column="bankftp_path"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="BankftpIp"
			column="bankftp_ip"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="BankftpPort"
			column="bankftp_port"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="BankftpUsername"
			column="bankftp_username"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="BankftpPassword"
			column="bankftp_password"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Whether_T_1"
			column="whether_t_1"
			type="boolean"
			not-null="false"
		/>
		<property
			name="WhetherParseBrank"
			column="whether_parse_brank"
			type="boolean"
			not-null="false"
		/>
		<many-to-one
			name="Bank"
			column="bank_id"
			class="BankInst"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>